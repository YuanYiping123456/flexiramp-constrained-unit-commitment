{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "One Dark italic",
                "scope": [
                    "comment",
                    "entity.other.attribute-name",
                    "keyword",
                    "markup.underline.link",
                    "storage.modifier",
                    "storage.type",
                    "string.url",
                    "variable.language.super",
                    "variable.language.this"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "name": "One Dark italic reset",
                "scope": [
                    "keyword.operator",
                    "keyword.other.type",
                    "storage.modifier.import",
                    "storage.modifier.package",
                    "storage.type.built-in",
                    "storage.type.function.arrow",
                    "storage.type.generic",
                    "storage.type.java",
                    "storage.type.primitive"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Night Owl Light]": {
            "textMateRules": [
                {
                    "name": "Comment",
                    "scope": [
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#bfdac0",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Keyword",
                    "scope": [
                        "keyword",
                        "storage",
                        "emphasis"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#e94343"
                    }
                },
                {
                    "name": "Function",
                    "scope": [
                        "entity.name.function",
                        "support.function"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#1b7cd6"
                    }
                },
                {
                    "name": "Parameter",
                    "scope": "variable.parameter",
                    "settings": {
                        "fontStyle": "underline"
                    }
                },
                {
                    "name": "Number",
                    "scope": [
                        "constant.numeric"
                    ],
                    "settings": {
                        "foreground": "#94a11b"
                    }
                }
            ]
        },
